    </formats>

    <fileSets>
        <fileSet>
            <directory>src/main/resources/assembly/bin</directory>
            <outputDirectory>MeetingRoomAdmin/bin</outputDirectory>
            <includes>
                <include>**/**</include>
            </includes>
            <directoryMode>755</directoryMode>
