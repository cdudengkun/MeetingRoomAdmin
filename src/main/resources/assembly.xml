        </fileSet>

        <fileSet>
            <directory>src/main/resources/assembly</directory>
            <outputDirectory>MeetingRoomAdmin</outputDirectory>
            <includes>
                <include>**/**</include>
            </includes>
        </fileSet>

        <fileSet>
            <directory>target</directory>
            <outputDirectory>MeetingRoomAdmin/lib</outputDirectory>
            <includes>
                <include>*.jar</include>
            </includes>
        </fileSet>
    </fileSets>
</assembly>
